; Engine configuration file.
; It's best edited using the editor UI and not directly,
; since the parameters that go here are not all obvious.
;
; Format:
;   [section] ; section goes between []
;   param=value ; assign values to parameters

config_version=5

[application]

config/name="Godot Wild Jam 61"
config/tags=PackedStringArray("in_progress")
run/main_scene="res://code/launcher.tscn"
config/features=PackedStringArray("4.1", "GL Compatibility")
config/icon="res://icon.svg"

[audio]

driver/mix_rate.web=44100

[autoload]

GlobalPopupSpace="*res://code/globals/singletones/global_popup_space.tscn"
GlobalSounds="*res://code/globals/singletones/global_sounds.tscn"
DialogueManager="*res://addons/dialogue_manager/dialogue_manager.gd"
LevelUnlocker="*res://code/globals/singletones/level_unlocker.tscn"

[display]

window/size/viewport_width=480
window/size/viewport_height=270
window/stretch/mode="canvas_items"

[editor_plugins]

enabled=PackedStringArray("res://addons/dialogue_manager/plugin.cfg", "res://addons/simplelicense/plugin.cfg")

[filesystem]

import/blender/enabled=false

[internationalization]

locale/translations_pot_files=PackedStringArray("res://resources/dialogues/dialogue_example.dialogue")

[rendering]

textures/canvas_textures/default_texture_filter=0
renderer/rendering_method="gl_compatibility"
renderer/rendering_method.mobile="gl_compatibility"
2d/snap/snap_2d_transforms_to_pixel=true
2d/snap/snap_2d_vertices_to_pixel=true
